{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.awaken_husk.controller": {
			"states": {
				"default": {
					"transitions": [
						{"throw": "q.variant == 1"},
						{"throw_larger": "q.variant == 2"}
					],
					"blend_transition": 0.25
				},
				"throw": {
					"animations": ["throw"],
					"transitions": [
						{"default": "q.variant == 0"}
					],
					"blend_transition": 0.5
				},
				"throw_larger": {
					"animations": ["throw_larger"],
					"transitions": [
						{"default": "q.variant == 0"}
					],
					"blend_transition": 0.5
				}
			}
		}
	}
}