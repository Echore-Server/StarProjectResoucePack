{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.grassland_zombie.controller": {
			"states": {
				"default": {
					"transitions": [
						{"spin": "q.variant == 1"},
						{"meteor": "q.variant == 2"}
					],
					"blend_transition": 0.5
				},
				"spin": {
					"animations": [
						{"spin": "1"}
					],
					"transitions": [
						{"default": "q.variant == 0"}
					]
				},
				"meteor": {
					"animations": [
						{"meteor": "1"}
					],
					"transitions": [
						{"default": "q.variant == 0"}
					],
					"blend_transition": 0.5
				}
			}
		}
	}
}